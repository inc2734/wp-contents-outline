{
  "author": "inc2734",
  "license": "GPL-2.0+",
  "dependencies": {
    "jquery.contents-outline": "^0.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-loader": "^8.0.4",
    "cpy-cli": "^2.0.0",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2"
  },
  "scripts": {
    "clean:js": "rimraf src/assets/js",
    "packages:jquery.contents-outline": "cpy 'jquery.contents-outline/**' '../src/assets/packages' --cwd=node_modules --parents",
    "packages": "npm-run-all -p packages:*",
    "js:build": "webpack src/src/js/wp-contents-outline.js -o src/assets/js/wp-contents-outline.min.js --config webpack.config.js",
    "js": "npm-run-all -s clean:js js:build",
    "start": "npm run build",
    "build": "npm run js && npm run packages",
    "postinstall": "npm run build"
  }
}
